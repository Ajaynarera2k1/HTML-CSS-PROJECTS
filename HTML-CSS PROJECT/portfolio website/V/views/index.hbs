    box-shadow: 5px 5px 7px 0px #0000003f;
    font-size: 18px;
    cursor: pointer;
    transition: all 0.5s;
    font-weight: 500;
    border: solid 3px transparent;
    position: relative;
    z-index: 1;
  }
  
  .btn::before {
    content: "";
    position: absolute;
    background-color: #fff;
    top: 0px;
    left: 0;
    right: 0;
    bottom: 0px;
    z-index: -1;
    transform: scaleX(0);
    transform-origin: left;
    transition: all 0.8s;
  }
  
  .btn:hover::before {
      transform: scaleX(1);
}
  
  .btn:hover {
    border: solid 3px #e84949;
    color: black;
  }
  .hero-sec-right{
      position: relative;
  }
  
  .absolute{
    position: absolute;
  }
  .UserImage{
     
      padding: 2.3rem;
      filter: grayscale(1);
      transition: all 3s;
      animation: scaleimage 5s linear infinite;
      
  }
  .UserImage img{
      z-index: -9;
      width: 370px;
  }

  @keyframes scaleimage{
    0%{
         filter: grayscale(0);
         transform: scale(1);
         
    }
    50%{
        filter: grayscale(1);
        transform: scale(1.08);
        box-shadow: 0px 0px 10px black;
        
        
    }
    100%{
        
        filter: grayscale(0);
        transform: scale(1);
        
    }
  }
  .icon-dots{
    z-index: 9;
    right: 0;
    bottom: -1rem;
    animation-name: dotsanimation;
    animation-duration: 4s;
    animation-iteration-count: infinite;
  }

  @keyframes dotsanimation{
    0%{
         transform: translateY(0px);
    }
    50%{
        transform: translateY(-15px);
    }
    100%{
        transform: translateY(0px);
    }
  }

  .icon-cube{
    z-index: 9;
    right: 1rem;
    top: 0.5rem;
    animation-name: cubemove;
    animation-duration: 4s;
    animation-iteration-count: infinite;
  }

  @keyframes cubemove{
    0%{
         transform: translatey(0px) rotatez(0deg); 
    }
    50%{
        transform: translatey(15px) rotatez(180deg); 
    }
    
    100%{
        transform: translatey(0px) rotatez(360deg); 
    }
}  
  
.icon-circle{
    z-index: 9;
    left: 0;
    bottom: 0;
    animation: circlemove 6s ease-in infinite;
}

@keyframes circlemove{
    0%{
           left: 0;
           bottom: 0;
    }
    50%{
        left: 5%;
        bottom: 10%;
    }
    
    100%{
        left: 0;
        bottom: 0;
    }
}

.icon-zigzags{
    z-index: 9;
    left: 0.8rem;
    top: 1.3rem;
